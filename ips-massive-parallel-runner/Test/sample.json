{
    "scan" : {
        "r"       : {"type" : "range", "ymin" : 1.4,  "ymax" : 2.2 },
        "aratio"  : {"type" : "range", "ymin" : 2.33, "ymax" : 3.5 },
        "kappa"   : {"type" : "range", "ymin" : 1.7,  "ymax" : 2.0 },
        "delta"   : {"type" : "range", "ymin" : 0.5,  "ymax" : 0.65},
