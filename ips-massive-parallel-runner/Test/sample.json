        "ti_axis"   : ["expr", "5.0 * ti_ped"             ],
        "ngw"       : ["base", {}                         ],
        "ne_ped"    : ["expr", "fgw_ped * ngw"            ],
        "ne_axis"   : ["expr", "nepeak * ne_ped"          ],
        "ne_sep"    : ["expr", "f_nesep * ne_ped"         ]
    },

    "io" : {
        "r"           : ["fastran_init", "r0"         ],
        "a"           : ["fastran_init", "a0"         ],
        "kappa"       : ["fastran_init", "kappa"      ],
        "delta"       : ["fastran_init", "delta"      ],
        "bt"          : ["fastran_init", "b0"         ],
        "ip"          : ["fastran_init", "ip"         ],
        "betan_ped"   : ["fastran_init", "betan_ped"  ],
        "xmid"        : ["fastran_init", "xmid"       ],
        "xwid"        : ["fastran_init", "xwid"       ],
        "ne_axis"     : ["fastran_init", "ne_axis"    ],
        "ne_ped"      : ["fastran_init", "ne_ped"     ],
        "ne_sep"      : ["fastran_init", "ne_sep"     ],
        "ne_alpha"    : ["fastran_init", "ne_alpha"   ],
        "ne_beta"     : ["fastran_init", "ne_beta"    ],
        "te_axis"     : ["fastran_init", "te_axis"    ],
        "te_ped"      : ["fastran_init", "te_ped"     ],
        "te_sep"      : ["fastran_init", "te_sep"     ],
        "te_alpha"    : ["fastran_init", "te_alpha"   ],
        "te_beta"     : ["fastran_init", "te_beta"    ],
        "ti_axis"     : ["fastran_init", "ti_axis"    ],
        "ti_ped"      : ["fastran_init", "ti_ped"     ],
        "ti_sep"      : ["fastran_init", "ti_sep"     ],
        "ti_alpha"    : ["fastran_init", "ti_alpha"   ],
        "ti_beta"     : ["fastran_init", "ti_beta"    ],
        "omega_axis"  : ["fastran_init", "omega_axis" ],
        "omega_sep"   : ["fastran_init", "omega_sep"  ],
        "omega_alpha" : ["fastran_init", "omega_alpha"],
        "omega_beta"  : ["fastran_init", "omega_beta" ],
        "zeff_axis"   : ["fastran_init", "zeff_axis"  ],
        "fgw_ped"     : ["fastran_init", "fgw_ped"    ]
