        "omega_axis"  : 0.0,
        "omega_sep"   : 0.0,
        "omega_alpha" : 2.0,
        "omega_beta"  : 2.0
    },

    "model" : {
        "a"         : ["expr", "r / aratio"               ],
        "betan_ped" : ["base", {}                         ],
        "te_ped"    : ["base", {"dependency":"betan_ped"} ],
        "ti_ped"    : ["expr", "te_ped"                   ],
