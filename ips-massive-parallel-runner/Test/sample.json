        "ip"      : {"type" : "range", "ymin" : 4.0,  "ymax" : 7.0 },
        "fgw_ped" : {"type" : "range", "ymin" : 0.2,  "ymax" : 1.0 },
        "nepeak"  : {"type" : "range", "ymin" : 1.5,  "ymax" : 2.0 }
    },

    "const" : {
        "xmid"        : 0.97,
        "xwid"        : 0.06,
        "ne_alpha"    : 1.5,
        "ne_beta"     : 1.1,
